@model Performance_Appraisal_System.Models.Report52

@{
    ViewBag.Title = "शासन वरिष्ठ कार्यालाकडून प्राप्त तक्रारी";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    Html.RenderPartial("PrevNextBar");
    Html.RenderPartial("ReadOnlyLayoutHeader");
}


<div id="WrappperDiv">

       <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">शासन वरिष्ठ कार्यालाकडून प्राप्त तक्रारी</span>
                    <hr>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Last_Month_Pending_Cases)
                            @Html.TextBoxFor(temp => temp.Last_Month_Pending_Cases, new { id = "Last_Month_Pending_Cases", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.Last_Month_Pending_Cases)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Current_Month_Received_Cases)
                            @Html.TextBoxFor(temp => temp.Current_Month_Received_Cases, new { id = "Current_Month_Received_Cases", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.Current_Month_Received_Cases)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Total_Cases)
                            @Html.TextBoxFor(temp => temp.Total_Cases, new { id = "Total_Cases_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.Total_Cases)
                            @Html.HiddenFor(temp => temp.Total_Cases, new { id = "Total_Cases" })
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">उद्दिष्ट  (चालु महिन्यात 45 दिवस पूर्ण होत असलेल्या तक्रारींची संख्या.)</span>
                    <hr>
                    <div class="form-row">

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Last_Month_Cases_Aim)
                            @Html.TextBoxFor(temp => temp.Last_Month_Cases_Aim, new { id = "Last_Month_Cases_Aim", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.Last_Month_Cases_Aim)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Current_Month_Cases_Aim)
                            @Html.TextBoxFor(temp => temp.Current_Month_Cases_Aim, new { id = "Current_Month_Cases_Aim", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.Current_Month_Cases_Aim)
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Total_Cases_Aim)
                            @Html.TextBoxFor(temp => temp.Total_Cases_Aim, new { id = "Total_Cases_Aim_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.Total_Cases_Aim)
                            @Html.HiddenFor(temp => temp.Total_Cases_Aim, new { id = "Total_Cases_Aim" })
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">साध्य </span>
                    <hr>
                    <div class="form-row">

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Last_Month_Achieved)
                            @Html.TextBoxFor(temp => temp.Last_Month_Achieved, new { id = "Last_Month_Achieved", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.Last_Month_Achieved)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Current_Month_Achieved)
                            @Html.TextBoxFor(temp => temp.Current_Month_Achieved, new { id = "Current_Month_Achieved", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.Current_Month_Achieved)
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Total_Achieved)
                            @Html.TextBoxFor(temp => temp.Total_Achieved, new { id = "Total_Achieved_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.Total_Achieved)
                            @Html.HiddenFor(temp => temp.Total_Achieved, new { id = "Total_Achieved" })
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">मुल्यांकनानुसार प्राप्त गुण</span>
                    <hr>
                    <div class="form-row">

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Appraisal_Marks)
                            @Html.TextBoxFor(temp => temp.Appraisal_Marks, new { id = "Appraisal_Marks_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.Appraisal_Marks)
                            @Html.HiddenFor(temp => temp.Appraisal_Marks, new { id = "Appraisal_Marks" })
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.Appraisal_Percentage)
                            @Html.TextBoxFor(temp => temp.Appraisal_Percentage, new { id = "Appraisal_Percentage_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.Appraisal_Percentage)
                            @Html.HiddenFor(temp => temp.Appraisal_Percentage, new { id = "Appraisal_Percentage" })
                        </div>

                        @*<div class="form-group col-md-10">
                                @Html.LabelFor(temp => temp.Remarks)
                                @Html.TextAreaFor(temp => temp.Remarks, new { id = "Remarks", @class = "form-control" })
                                @Html.ValidationMessageFor(temp => temp.Remarks)
                            </div>*@

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@{
    Html.RenderPartial("PrintButtonBar");
}


<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<script>
    $(document).ready()
    {
        $('input,textarea').attr('disabled', true);
$('#Button').prop('disabled', false);
    }

</script>