
@{
    ViewBag.Title = "SubReports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div style="text-align:center; padding:30px;">
    <h5><strong>जिल्हा उपनिबंधक सहकारी संस्था व त्या अंतर्गत असलेले उपनिबंधक कार्यालये यांसाठी अधिकाऱ्यांचा मूल्यांक भारांक तक्ता खालीलप्रमाणे </strong></h5>



    <hr>

    <table class="table table-hover branchtable" style=" margin:auto; width:85%;">
        <thead style="text-align:center; vertical-align:middle !important;">
            <tr>
                <th scope="col">विषय</th>
                <th scope="col">गुण</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var department in Model)
            {
                <tr>
                    <td style="vertical-align: middle !important">@department.SubjectName</td>
                    <td style="vertical-align: middle !important"><a href="/Report/SubReports?department=@department.DepartmentId&month=1&year=2020" title="Open" class="btn btn-success">Open</a></td>
                </tr>
            }

            <tr style="text-align:center">
                <td colspan="3"><strong>एकुण</strong></td>
                <td><strong>100</strong></td>
            </tr>

        </tbody>
    </table>
</div>

