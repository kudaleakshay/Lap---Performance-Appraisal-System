<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -सहकार आयुक्त आणि निबंधक</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/fevicon.ico" />
    @Styles.Render("~/Content/css")
    <link href="~/Content/css/jquery.dataTables.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="page-wrapper">

        <div class="top-header">
            <div class="left-top-header">
                <div class="top-search-box">
                    <input type="search" class="form-control" placeholder="येथे शोधा">
                    <i class="fa fa-search"></i>
                </div>
            </div>
            <div class="right-top-header">
                <ul>
                    @if (Session["UserName"] != null)
                    {
        <li><a class="dropdown-toggle" href="/Account/Logout" id="loginLink">लॉग ऑऊट</a></li>
 }
    else
    {
<li><a class="dropdown-toggle" href="/Account/Login" id="loginLink">लॉग इन</a></li>
}

                    <li><a href="/">मुख्य पृष्ठ</a></li>
                    <li>संपर्क</li>
                    <li><a href="#mainmenu">दिशादर्शकाकडे जा</a></li>
                    <li><a href="#content">विषयाकडे जा</a></li>
                    <li>अ--</li>
                    <li>अ-</li>
                    <li>अ+</li>
                    <li>अ++</li>
                    <li>English</li>
                </ul>
            </div>
        </div>

        <div class="siteHeader">
            <a href="../Home/" class="mahalogo"><img src="../../Images/maharashtralogo.png" id="HeaderMain1_MaharashtraLogo_alt" alt="महाराष्ट्र शासन"></a>
            <span id="HeaderMain1_lbl_headTitle">
                सहकार आयुक्त आणि निबंधक, सहकारी संस्था,<br>
                महाराष्ट्र राज्य, पुणे
            </span>
            <a href="http://india.gov.in/" class="indiagov ExternalLinkClick" target="_blank"><img src="../../Images/emblem.png" id="HeaderMain1_NationalEmblem_alt" alt="भारत सरकार"></a>
        </div>

        <div class="top-menu navbar-collapse collapse monitor-menu ">
            <ul id="mainmenu" class="nav navbar-nav" style="background: #368bd6;box-shadow: 0px 6px 5px -2px rgba(0,0,0,0.31);z-index: 999;position: relative;">
                <li class="dropdown dropdown-quick-sidebar-toggler">
                    <a href="/"> मुखपृष्ठ </a>
                </li>
                <li class="dropdown dropdown-quick-sidebar-toggler">
