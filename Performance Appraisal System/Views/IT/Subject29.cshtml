@model Performance_Appraisal_System.Models.Report29

@{
    ViewBag.Title = "CM / PM Portal वरील प्राप्त तक्रारींचा आढावा";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br>
<br>
@using (Html.BeginForm("Subject29", "O_M", FormMethod.Post))
{
    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <br>
            <br>
            <h3 class="card-title text-center"><strong>विभागीय सहनिबंधक, जिल्हा उपनिबंधक व त्या अंतर्गत असलेले उपनिबंधक कार्यालये यांसाठी अधिकाऱ्यांचा मूल्यांक भारांक तक्ता</strong></h3>
            <hr>
            <div class="card-text">
                <div class="center-group">
                    <div class="form-row">
                        <div class="form-group col-md-12 center-div" style="color: #e77817 !important; font-size: 20px;">
                            कार्यासनाचे नाव: <strong> @Session["ReportDepartmentName"].ToString() </strong> &nbsp&nbsp&nbsp|&nbsp&nbsp&nbsp विषय: <strong> कार्यालय तपासणी </strong>
                            &nbsp&nbsp&nbsp|&nbsp&nbsp&nbsp<strong> गुण:  @Session["TotalMarks"].ToString() </strong>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.DropDownListFor(model => model.Month, (IEnumerable<SelectListItem>)ViewBag.Months, "-- महिना निवडा --", new { @class = "form-control" })
                            <br>
                            @Html.ValidationMessageFor(temp => temp.Month)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.DropDownListFor(model => model.Year, (IEnumerable<SelectListItem>)ViewBag.Years, "-- वर्ष निवडा --", new { @class = "form-control" })
                            <br>
                            @Html.ValidationMessageFor(temp => temp.Year)
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>


    <div class="form-group col-md-12 center-div" style="font-size: 20px;">
        @Html.CheckBoxFor(temp => temp.NotApplicable, new { id = "NotApplicable" })
        <strong>&nbsp&nbspविषय लागु नाही</strong>
    </div>

    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">CM Portal वरील तक्रारींची माहिती</span>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.CM_Last_Month_Pending_Complaints)
                            @Html.TextBoxFor(temp => temp.CM_Last_Month_Pending_Complaints, new { id = "CM_Last_Month_Pending_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.CM_Last_Month_Pending_Complaints)
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.CM_Current_Month_Received_Complaints)
                            @Html.TextBoxFor(temp => temp.CM_Current_Month_Received_Complaints, new { id = "CM_Current_Month_Received_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.CM_Current_Month_Received_Complaints)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.CM_Total_Complaints)
                            @Html.TextBoxFor(temp => temp.CM_Total_Complaints, new { id = "CM_Total_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.CM_Total_Complaints)
                        </div>

                        <hr>
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.CM_Current_Month_Resolved_Complaints)
                            @Html.TextBoxFor(temp => temp.CM_Current_Month_Resolved_Complaints, new { id = "CM_Current_Month_Resolved_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.CM_Current_Month_Resolved_Complaints)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.CM_Pending_Complaints)
                            @Html.TextBoxFor(temp => temp.CM_Pending_Complaints, new { id = "CM_Pending_Complaints_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.CM_Pending_Complaints)
                            @Html.HiddenFor(temp => temp.CM_Pending_Complaints, new { id = "CM_Pending_Complaints" })
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">PM Portal वरील तक्रारींची माहिती</span>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.PM_Last_Month_Pending_Complaints)
                            @Html.TextBoxFor(temp => temp.PM_Last_Month_Pending_Complaints, new { id = "PM_Last_Month_Pending_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.PM_Last_Month_Pending_Complaints)
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.PM_Current_Month_Received_Complaints)
                            @Html.TextBoxFor(temp => temp.PM_Current_Month_Received_Complaints, new { id = "PM_Current_Month_Received_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.PM_Current_Month_Received_Complaints)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.PM_Total_Complaints)
                            @Html.TextBoxFor(temp => temp.PM_Total_Complaints, new { id = "PM_Total_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.PM_Total_Complaints)
                        </div>

                        <hr>
                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.PM_Current_Month_Resolved_Complaints)
                            @Html.TextBoxFor(temp => temp.PM_Current_Month_Resolved_Complaints, new { id = "PM_Current_Month_Resolved_Complaints", @class = "form-control" })
                            @Html.ValidationMessageFor(temp => temp.PM_Current_Month_Resolved_Complaints)
                        </div>

                        <div class="form-group col-md-6">
                            @Html.LabelFor(temp => temp.PM_Pending_Complaints)
                            @Html.TextBoxFor(temp => temp.PM_Pending_Complaints, new { id = "PM_Pending_Complaints_val", @class = "form-control", disabled = "disabled" })
                            @Html.ValidationMessageFor(temp => temp.PM_Pending_Complaints)
                            @Html.HiddenFor(temp => temp.PM_Pending_Complaints, new { id = "PM_Pending_Complaints" })
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 16px;">मुल्यांकनानुसार प्राप्त गुण</span>
                    <hr>


                    <div class="form-group col-md-6">
                        @Html.LabelFor(temp => temp.Appraisal_Marks)
                        @Html.TextBoxFor(temp => temp.Appraisal_Marks, new { id = "Appraisal_Marks_val", @class = "form-control", disabled = "disabled" })
                        @Html.ValidationMessageFor(temp => temp.Appraisal_Marks)
                        @Html.HiddenFor(temp => temp.Appraisal_Marks, new { id = "Appraisal_Marks" })
                    </div>

                    <div class="form-group col-md-6">
                        @Html.LabelFor(temp => temp.Appraisal_Percentage)
                        @Html.TextBoxFor(temp => temp.Appraisal_Percentage, new { id = "Appraisal_Percentage_val", @class = "form-control", disabled = "disabled" })
                        @Html.ValidationMessageFor(temp => temp.Appraisal_Percentage)
                        @Html.HiddenFor(temp => temp.Appraisal_Percentage, new { id = "Appraisal_Percentage" })
                    </div>

                    <div class="form-group col-md-10">
                        @Html.LabelFor(temp => temp.Remarks)
                        @Html.TextAreaFor(temp => temp.Remarks, new { id = "Remarks", @class = "form-control" })
                        @Html.ValidationMessageFor(temp => temp.Remarks)
                    </div>
                    <br>
                </div>
            </div>
        </div>
    </div>


    <div class="container" style="padding: 30px;background: white;">
        <div class="col-md-12" style="box-shadow: 0px 0px 5px lightgrey;">
            <div class="card-text">
                <div class="center-group">
                    <br>
                    <span style="color: #e77817 !important; font-size: 17px;">
                        Confirmation / पुष्टीकरण
                    </span>
                    <hr>

                    <div style="text-align:justify; font-size: 16px; line-height:25px">
                        I affirm that the above mentioned information submitted by me is true and correct to my knowledge and belief. / मी प्रमाणित करतो की मी वर नमूद केलेली माहिती माझ्या माहितीनुसार सत्य आणि अचूक आहे.
                    </div>
                    <br>
                    <div class="form-group">
                        <button type="submit" class="btn btn-success" OnClick="SetValue()">Submit / माहिती भरा</button>
                    </div>
                    <br>
                </div>
            </div>
        </div>
    </div>


    <br>
    <br>


}

<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<script>

    function Total_Staff() {
        var Total = Number($("#Officer_Count").val()) + Number($("#Staff_Count").val());
        $("#Total_Staff").val(Total);
        $("#Total_Staff_val").val(Total);
        $("#Total_Staff_val").css("font-weight", "bold");
    }

    function Total_Target() {
        var Total = Number($("#Officer_Target").val()) + Number($("#Staff_Target").val());
        $("#Total_Target").val(Total);
        $("#Total_Target_val").val(Total);
        $("#Total_Target_val").css("font-weight", "bold");
    }

    function CM_Pending_Complaints() {
        var Total = Number($("#CM_Current_Month_Received_Complaints").val()) + Number($("#CM_Total_Complaints").val());
        $("#CM_Pending_Complaints").val(Total);
        $("#CM_Pending_Complaints_val").val(Total);
        $("#CM_Pending_Complaints_val").css("font-weight", "bold");
    }



    function Appraisal_Marks() {
        var Total = ((Number($("#CM_Pending_Complaints").val()) * 5) / Number($("#Total_Target").val()));
        Total = Total.toFixed(2);
        $("#Appraisal_Marks").val(Number(Total));
        $("#Appraisal_Marks_val").val(Number(Total));
        $("#Appraisal_Marks_val").css("font-weight", "bold");
    }

    function Appraisal_Percentage() {
        var Total = (Number($("#Appraisal_Marks").val()) * 100 / 5);
        Total = Total.toFixed(2);
        $("#Appraisal_Percentage").val(Number(Total));
        $("#Appraisal_Percentage_val").val(Number(Total));
        $("#Appraisal_Percentage_val").css("font-weight", "bold");
    }

    function SetValue() {
        $("#Appraisal_Percentage").val(Number($("#Appraisal_Percentage_val").val()));
        $("#Appraisal_Marks").val(Number($("#Appraisal_Marks_val").val()));
        $("#Current_Month_Percentage").val(Number($("#Current_Month_Percentage_val").val()));
    }


    $("#Officer_Count").change(Total_Staff);
    $("#Staff_Count").change(Total_Staff);

    $("#Officer_Target").change(Total_Target);
    $("#Staff_Target").change(Total_Target);

    $("#CM_Current_Month_Received_Complaints").change(CM_Pending_Complaints);
    $("#CM_Total_Complaints").change(CM_Pending_Complaints);

    $("#CM_Last_Month_Pending_Complaints").change(Current_Month_Percentage);
    $("#CM_Current_Month_Received_Complaints").change(Current_Month_Percentage);
    $("#CM_Total_Complaints").change(Current_Month_Percentage);

    $("#Officer_Target").change(Appraisal_Marks);
    $("#Staff_Target").change(Appraisal_Marks);
    $("#CM_Current_Month_Received_Complaints").change(Appraisal_Marks);
    $("#CM_Total_Complaints").change(Appraisal_Marks);


    $("#Officer_Target").change(Appraisal_Percentage);
    $("#Staff_Target").change(Appraisal_Percentage);
    $("#CM_Current_Month_Received_Complaints").change(Appraisal_Percentage);
    $("#CM_Total_Complaints").change(Appraisal_Percentage);

</script>
